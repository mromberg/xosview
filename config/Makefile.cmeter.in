#-------------------------------------------------------------------------
# @configure_input@
# Any edits of this file will be lost
# the next time configure is run.
#
# Common meter Makefile
#
#-------------------------------------------------------------------------

include ../Makefile.config


#-------------------------------------------------------------------------
# File lists
#-------------------------------------------------------------------------
OBJS = \
cswapmeter.o \
pswapmeter.o \
cloadmeter.o \
ploadmeter.o \
cintratemeter.o \
pintratemeter.o \
cfsmeter.o \
cnetmeter.o \
pnetmeter.o \
cpagemeter.o \
ppagemeter.o \
ctzonemeter.o \
cdiskmeter.o \
pdiskmeter.o \
cintmeter.o \
cmetermaker.o


CFILES := $(OBJS:.o=.cc)
DEPFILES := $(OBJS:%=.%.d)


#-----------------------------------------------------------------------
# Targets
#-----------------------------------------------------------------------

all : libcmeter.a

libcmeter.a : $(OBJS)
	ar cvr libcmeter.a $(OBJS)
	@RANLIB@ $@

clean :
	rm -f $(OBJS) $(DEPFILES) libcmeter.a *~

# auto dependency makefile fragments
@AUTODEPEND@
