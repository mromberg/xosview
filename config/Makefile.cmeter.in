include ../Makefile.config
@AUTODEPRULE_SUBDIR@

#arch=$(shell uname -m)
#export arch

#CXXFLAGS += -I$(TOP)

OBJS = \
cswapmeter.o \
pswapmeter.o \
cloadmeter.o \
ploadmeter.o \
cintratemeter.o \
pintratemeter.o \
cfsmeter.o \
cnetmeter.o \
pnetmeter.o \
cpagemeter.o \
ppagemeter.o \
ctzonemeter.o \
cdiskmeter.o \
pdiskmeter.o


CFILES := $(OBJS:.o=.cc)
DEPFILES := $(OBJS:%=.%.d)


#-----------------------------------------------------------------------

all : libcmeter.a


clean :
	rm -f $(OBJS) $(DEPFILES) libcmeter.a *~

libcmeter.a : $(OBJS)
	ar cvr libcmeter.a $(OBJS)
	@RANLIB@ $@

@AUTODEPEND@
