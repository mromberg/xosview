#-------------------------------------------------------------------------
# @configure_input@
# Any edits of this file will be lost
# the next time configure is run.
#
# GNU/hurd Makefile
#
#-------------------------------------------------------------------------
include ../Makefile.config

#-------------------------------------------------------------------------
# File lists
#-------------------------------------------------------------------------
OBJS = \
cpumeter.o \
memmeter.o \
get_def_pager.o \
MeterMaker.o

CFILES := $(OBJS:.o=.cc)
DEPFILES := $(OBJS:%=.%.d)

#-----------------------------------------------------------------------
# Rules
#-----------------------------------------------------------------------

all : libmeter.a

libmeter.a : $(OBJS)
	ar cvr libmeter.a $(OBJS)
	@RANLIB@ $@

clean :
	rm -f $(OBJS) $(DEPFILES) libmeter.a *~

# Automatic dependencies
@AUTODEPEND@
