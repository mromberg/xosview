include ../Makefile.config


#-------------------------------------------------------------------------
# File lists
#-------------------------------------------------------------------------

OBJS = \
btrymeter.o \
cpumeter.o \
diskmeter.o \
intmeter.o \
intratemeter.o \
kernel.o \
loadmeter.o \
memmeter.o \
netmeter.o \
pagemeter.o \
swapmeter.o \
fsmeter.o \
sensor.o \
coretemp.o \
MeterMaker.o


CFILES := $(OBJS:.o=.cc)
DEPFILES := $(OBJS:%=.%.d)

#-----------------------------------------------------------------------
# Rules
#-----------------------------------------------------------------------

all : libmeter.a # concatdepend

libmeter.a : $(OBJS)
	ar cvr libmeter.a $(OBJS)
	@RANLIB@ $@

clean :
	rm -f $(OBJS) $(DEPFILES) libmeter.a *~


# Auto depend support
@AUTODEPEND@
