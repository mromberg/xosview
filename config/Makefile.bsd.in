include ../Makefile.config
-include ../Makefile.GNU.autodep

CXXFLAGS += -I@top_srcdir@

OBJS = \
MeterMaker.o \
cpumeter.o \
memmeter.o \
swapmeter.o \
swapinternal.o \
netmeter.o \
loadmeter.o \
diskmeter.o \
pagemeter.o \
kernel.o

#  These next two are not yet supported in the NetBSD port.
#intmeter.o \
#serialmeter.o \

CFILES := $(OBJS:.o=.cc)
DEPFILES := $(OBJS:%=.%.d)

#-----------------------------------------------------------------------

all : libmeter.a # concatdepend

clean :
	rm -f $(OBJS) $(DEPFILES) libmeter.a *~

libmeter.a : $(OBJS)
	ar cvr libmeter.a $(OBJS)
	@RANLIB@ $@

@AUTODEPEND@
