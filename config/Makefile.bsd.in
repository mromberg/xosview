include ../Makefile.config
@AUTODEPRULE_SUBDIR@

CXXFLAGS += -I@TOP_SRCDIR@


# No longer runs (kernel changes I think)
#swapinternal.o \
#diskmeter.o \
#intmeter.o \

OBJS = \
MeterMaker.o \
cpumeter.o \
memmeter.o \
swapmeter.o \
pagemeter.o \
netmeter.o \
loadmeter.o \
intratemeter.o \
kernel.o

CFILES := $(OBJS:.o=.cc)
DEPFILES := $(OBJS:%=.%.d)

#-----------------------------------------------------------------------

all : libmeter.a # concatdepend

clean :
	rm -f $(OBJS) $(DEPFILES) libmeter.a *~

libmeter.a : $(OBJS)
	ar cvr libmeter.a $(OBJS)
	@RANLIB@ $@

CXXFLAGS += -Wno-missing-field-initializers

@AUTODEPEND@
