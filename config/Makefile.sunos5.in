include ../Makefile.config
@AUTODEPRULE_SUBDIR@

CXXFLAGS += -I@top_srcdir@

OBJS = \
MeterMaker.o \
loadmeter.o \
cpumeter.o \
memmeter.o \
pagemeter.o \
swapmeter.o


CFILES := $(OBJS:.o=.cc)
DEPFILES := $(OBJS:%=.%.d)

vpath %.h $(TOP)
vpath %.cc $(TOP)

#-----------------------------------------------------------------------

all : libmeter.a

clean :
	rm -f $(OBJS) $(DEPFILES) libmeter.a *~

libmeter.a : $(OBJS)
	ar cvr libmeter.a $(OBJS)
	@RANLIB@ $@

@AUTODEPEND@
