include ../Makefile.config
@AUTODEPRULE_SUBDIR@

CXXFLAGS += -I$(TOP)

OBJS = \
MeterMaker.o \
loadmeter.o \
cpumeter.o \
gfxmeter.o \
sarmeter.o \
memmeter.o \
diskmeter.o


CFILES := $(OBJS:.o=.cc)
DEPFILES := $(OBJS:%=.%.d)

vpath %.h $(TOP)
vpath %.cc $(TOP)

#-----------------------------------------------------------------------

all : libmeter.a

clean :
	rm -f $(OBJS) $(DEPFILES) libmeter.a *~

libmeter.a : $(OBJS)
	ar cvr libmeter.a $(OBJS)
	@RANLIB@ $@

@AUTODEPEND@
